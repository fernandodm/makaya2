<h1>Partido</h1>
<table class="table table-bordered">
	<tr>
		<th>Informacion sobre el partido</th>
	</tr>
	<% @partidos.each do | partido | %>

	<tr>
		<td>
			<%= partido.equipo1 %>
		</td>
		<td>
			<%= partido.resultado1 %>
		</td>
		<td>
			<%= partido.resultado2 %>
		</td>
		<td>
			<%= partido.equipo2 %>
		</td>
				
	</tr>

  <% end %>

</table>

<h1>Editar Equipo</h1>
<% form_for(@equipo, "/equipo/update/#{@equipo.id}") do |f| %>

  <%= partial 'equipo/form', :locals => { :f => f } %>

  <%= f.submit "Guardar", :class => "btn btn-primary" %>
  <%= link_to 'Cancelar', url_for(:equipo, :misEquipos, :torneo_id => @equipo.torneo.id), :class => "btn btn-secondary" %>
<% end %>

Makaya2::App.controllers :tabla do
	
	#include Comparable 

	get :misEquipos, :with =>:torneo_id do
    @torneo = Torneo.get(params[:torneo_id])
    @equipos = Equipo.find_by_torneo(@torneo)
    render 'tabla/list'
	end
=begin
	def <=>(anOther)
		if @pts != anOther.pts
			@pts <=> anOther.pts
		elsif @ganados != anOther.ganados
			@ganados <=> anOther.ganados
		else
			@empatados <=> anOther.empatados
		end	
	
	end
=end
end
